        <!-- API 11 theme customizations can go here. -->
        <item name="android:windowActionBar">false</item>
    </style>
    
    <style name="NP__.Theme.Dialog" parent="NP__Theme.DialogBase">
        <item name="android:windowActionBar">false</item>
    </style>

</resources>